num = {"1":"#\n#\n#\n#\n#",
       "2":"###\n  #\n###\n#\n###",
       "3":"###\n  #\n###\n  #\n###",
       "4":"# #\n# #\n###"+"\n  #"*3,
       "5":"###\n#  \n###\n  #\n###",
       "6":"###\n#\n###\n# #\n###",
       "7":"###"+"\n  #"*4,
        "8":"###\n# #\n###\n# #\n###",
       "9":"###\n# #\n###\n  #\n  #",
       "0":"###\n# #\n# #\n# #\n###"}
r =input("digite o nÃºmero q deseja ver no LED: ")

for i in list(r):
    if i in num:
        print(num[i])
